    <section id="contact_us_section" class="py-5 background1">
        <div class="container">
            <h3 class="text-center text-secondary">Contact Us</h3>
            <div class="row">
                <div class="col-md-12">
                    <form action="/process-contact-form" method="post">
                        <div class="mb-3">
                            <label for="exampleInputEmail1" class="form-label">Your Email address</label>
                            <input type="email" name="email" class="form-control" id="exampleInputEmail1"
                                aria-describedby="emailHelp">
                            <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
                        </div>
                        <div class="mb-3">
                            <label for="contactNumberField" class="form-label">Your Contact Number</label>
                            <input type="number" name="phone" class="form-control" id="exampleInputPassword1">
                        </div>

                        <div class="form-floating mt-2">
                            <textarea style="height: 220px;" name="query" class="form-control" placeholder="Leave a comment here"
                                id="floatingTextarea"></textarea>
                            <label for="floatingTextarea">Comments</label>
                        </div>

                        <div class="container text-center my-3">
                            <button type="submit" class="btn btn-dark">Submit</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>

